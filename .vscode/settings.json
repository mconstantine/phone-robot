{
  "editor.formatOnSave": true,
  "git.ignoreLimitWarning": true,
  "prettier.semi": false,
  "prettier.singleQuote": true,
  "prettier.trailingComma": "none",
  "prettier.arrowParens": "avoid",
  "typescript.tsdk": "node_modules/typescript/lib",
  "files.associations": {
    "*.tcc": "cpp",
    "bitset": "cpp",
    "optional": "cpp",
    "functional": "cpp",
    "fstream": "cpp",
    "istream": "cpp",
    "ostream": "cpp",
    "sstream": "cpp",
    "streambuf": "cpp"
  }
}
